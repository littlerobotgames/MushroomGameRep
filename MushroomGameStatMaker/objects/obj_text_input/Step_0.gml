if typing
	{
	switch type
		{
		case in_type.number:
			mytext = string_digits(keyboard_string);
			break;
		case in_type.text:
			mytext = string_letters(keyboard_string);
			break;
		case in_type.both:
			mytext = keyboard_string;
			break;
		}
			
		mytext = keyboard_string;
	}